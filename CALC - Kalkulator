#include <iostream>
 
using namespace std;
 
int main()
{
    char znak;
    int32_t A,B,C;
 
     while (cin >> znak >> A >> B)
    {
 
    if (znak=='+')
    {
        C=A+B;
        cout << C << endl;
    }
    if (znak=='-')
    {
        C=A-B;
        cout << C << endl;
    }
    if (znak=='*')
    {
        C=A*B;
        cout << C << endl;
    }
    if (znak=='/')
    {
        C=A/B;
        cout << C << endl;
    }
     if (znak=='%')
    {
        C=A%B;
        cout << C << endl;
    }
 
 }
    return 0;
} 
